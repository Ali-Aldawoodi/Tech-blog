
<div>
   

    {{#each blogPosts as |blogPost|}}
    <div class="blogpost-card">

        <div class="title">
            {{blogPost.title}}
        </div>

        <div class="content">
            {{blogPost.content}}
        </div>

        <div class="date">
            {{blogPost.date_created}}
            <button  class="delete"  data-id="{{blogPost.id}}">DELETE</button>
        </div>
    </div>
    {{/each}}

</div>

<script src="./js/homepage.js"></script>